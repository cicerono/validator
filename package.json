{
  "name": "@relekang/validator",
  "version": "0.4.2",
  "description": "",
  "main": "./lib/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib",
    "test": "nyc --reporter cobertura --reporter text ava",
    "lint": "eslint .",
    "semantic-release": "false && semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relekang/validator.git"
  },
  "author": "Rolf Erik Lekang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/relekang/validator/issues"
  },
  "homepage": "https://github.com/relekang/validator#readme",
  "ava": {
    "verbose": true,
    "require": [
      "babel-core/register"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "plugins": [
      "ava"
    ],
    "rules": {
      "ava/test-ended": 2,
      "semi": [
        2,
        "never"
      ]
    }
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-ava": "0.0.1",
    "nyc": "^5.5.0",
    "sinon": "^1.17.3"
  },
  "dependencies": {
    "lodash": "^4.12.0",
    "validator": "^4.5.1"
  }
}
