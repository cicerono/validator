{
  "name": "@relekang/validator",
  "version": "0.5.0",
  "description": "",
  "main": "./lib/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib",
    "test": "nyc -a --require babel-core/register --reporter cobertura --reporter text ava",
    "lint": "eslint .",
    "semantic-release": "false && semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relekang/validator.git"
  },
  "author": "Rolf Erik Lekang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/relekang/validator/issues"
  },
  "homepage": "https://github.com/relekang/validator#readme",
  "ava": {
    "verbose": true,
    "files": [
      "src/**/__tests__/*.js"
    ],
    "require": [
      "babel-core/register"
    ]
  },
  "nyc": {
    "exclude": [
      "**/__tests__",
      "lib"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:ava/recommended"
    ],
    "plugins": [
      "ava"
    ]
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-ava": "^2.3.1",
    "eslint-plugin-import": "^1.8.0",
    "nyc": "^6.4.4",
    "sinon": "^1.17.4"
  },
  "dependencies": {
    "deep-freeze": "0.0.1",
    "es6-error": "^3.0.0",
    "lodash": "^4.12.0",
    "validator": "^4.5.1"
  }
}
